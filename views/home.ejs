  <div class="container" style="margin-top: 30px;">
    <h1>Latest Stories</h1>
    <hr>
      <div class="row">
        <%  post.forEach(function(pst){ %>

        
          <div class="col col-10">
            <div class="card mb-3" style="max-width: 1000px;">
                <div class="row g-0">
                  <div class="col-md-4">
                    <img src="/uploads/<%=pst.image%>" class="card-img-top" alt="...">
                  </div>
                  <div class="col-md-8">
                    <div class="card-body">
                      <h5 class="card-title"><%=pst.title%></h5>
                      <p class="card-text"><%=(pst.desc).slice(0, 100)%> ...</p>
                      <p><a href="/posts/<%=pst._id%>">see more</a></p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          <%  }) %>
             

          
      </div>
  </div>
  
